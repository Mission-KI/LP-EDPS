{%- import "utils.html.jinja" as utils -%}

{% macro print(ds) -%}
    {{
        utils.print_dataset_table("Image", ds, [
            ("Codec", ds.details.codec),
            ("Color mode", ds.details.colorMode),
            ("Resolution", ds.details.resolution),
            ("DPI", ds.details.dpi),
            ("Brightness", ds.details.brightness),
            ("Blurriness", ds.details.blurriness),
            ("Sharpness", ds.details.sharpness),
            ("Brisque", ds.details.brisque),
            ("Noise", ds.details.noise),
            ("Low contrast", ds.details.lowContrast),
            ("ELA score", ds.details.elaScore),
        ])
    }}
{% endmacro %}
